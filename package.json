{
  "name": "unixcrypt",
  "version": "1.2.0",
  "description": "Node.js implementation of Unixcrypt, specifically SHA-256 and SHA-512",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "clean": "rimraf dist",
    "prep": "npm install && npm run prep:husky",
    "prep:husky": "husky install && chmod ug+x .husky/*",
    "prepublishOnly": "npm run build",
    "test": "npm run clean && jest --coverage --verbose",
    "test:watch": "npm run test -- --watchAll"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markusberg/unixcrypt.git"
  },
  "keywords": [
    "sha256crypt",
    "sha512crypt",
    "unixcrypt"
  ],
  "author": "Markus Berg",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/markusberg/unixcrypt/issues"
  },
  "homepage": "https://github.com/markusberg/unixcrypt#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^16.18.16",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
